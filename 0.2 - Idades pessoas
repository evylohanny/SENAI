programa {
  funcao inicio() {

cadeia nomePessoaUm , nomepessoaDois , nomePessoaTres , nomepessoaQuatro , nomePessoaCinco
inteiro idadePessoaUm , idadePessoaDois , idadePessoaTres , idadePessoaQuatro , idadePessoaCinco
real mediaIdades , mediaIdadesTotal
real relUm = 5

escreva("Digite um nome: ")
leia(nomePessoaUm)
escreva("Digite a idade dessa pessoa: ")
leia(idadePessoaUm)

escreva("\nDigite um nome: ")
leia(nomepessoaDois)
escreva("Digite a idade dessa pessoa: ")
leia(idadePessoaDois)

escreva("\nDigite um nome: ")
leia(nomePessoaTres)
escreva("Digite a idade dessa pessoa: ")
leia(idadePessoaTres)

escreva("\nDigite um nome: ")
leia(nomepessoaQuatro)
escreva("Digite a idade dessa pessoa: ")
leia(idadePessoaQuatro)

escreva("\nDigite um nome: ")
leia(nomePessoaCinco)
escreva("Digite a idade dessa pessoa: ")
leia(idadePessoaCinco)

mediaIdades = idadePessoaUm + idadePessoaDois + idadePessoaTres + idadePessoaQuatro + idadePessoaCinco
mediaIdadesTotal = mediaIdades / relUm

escreva("\nMÃ©dia de idades: " , mediaIdadesTotal)

se(idadePessoaUm > idadePessoaDois e idadePessoaUm > idadePessoaTres e idadePessoaUm > idadePessoaQuatro e idadePessoaUm > idadePessoaCinco){
  escreva("\n" , nomePessoaUm , " tem a maior idade")

}senao se(idadePessoaDois > idadePessoaUm e idadePessoaDois > idadePessoaTres e idadePessoaDois > idadePessoaQuatro e idadePessoaDois > idadePessoaCinco){
  escreva("\n" , nomepessoaDois , " tem a maior idade")

}se(idadePessoaTres > idadePessoaUm e idadePessoaTres > idadePessoaDois e idadePessoaTres > idadePessoaQuatro e idadePessoaTres > idadePessoaCinco){
  escreva("\n" , nomePessoaTres , " tem a maior idade")

}senao se(idadePessoaQuatro > idadePessoaUm e idadePessoaQuatro > idadePessoaDois e idadePessoaQuatro > idadePessoaTres e idadePessoaQuatro > idadePessoaCinco){
  escreva("\n" , nomepessoaQuatro , " tem a maior idade")

}se(idadePessoaCinco > idadePessoaUm e idadePessoaCinco > idadePessoaDois e idadePessoaCinco > idadePessoaTres e idadePessoaCinco > idadePessoaQuatro){
  escreva("\n" , nomePessoaCinco , " tem a maior idade")

}
}
}
